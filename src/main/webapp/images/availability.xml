<?xml version="1.0" encoding="UTF-8"?>
<persistence>  
<persistence-unit name="availability">  
	<provider>org.hibernate.ejb.HibernatePersistence</provider>
      <class>com.ketul.demo.model.Doctor</class>  
      <class>com.ketul.demo.model.Availability</class>  
      
  
        
     <properties>  
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>  
         <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/onlineAppointment"/>  
         <property name="javax.persistence.jdbc.user" value="root"/>  
         <property name="javax.persistence.jdbc.password" value="Drc@1234"/>  
         <property name="eclipselink.logging.level" value="SEVERE"/>  
         <property name="eclipselink.ddl-generation" value="create-or-extend-tables"/>  
      </properties>  
      
    </persistence-unit>  
</persistence>  
